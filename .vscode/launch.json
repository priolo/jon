	"version": "0.2.0",
	"configurations": [
		{
			"name": "all tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/react-scripts/bin/react-scripts.js",
			"args": [
				"test",
				"src/tests",
				"--verbose",
				"-i",
				"--no-cache"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		},
		{
			"name": "test Validators",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/react-scripts/bin/react-scripts.js",
			"args": [
				"test",
				"src/tests/validators.test.js",
				"--verbose",
				"-i",
				"--no-cache"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		},
		
	]
}
